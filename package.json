{
  "name": "youtube-top",
  "version": "1.0.0",
  "description": "> A web app that visualizes the most popular videos and channels on Youtube.",
  "main": "/server/app.js",
  "engines": {
    "node": "16.13.2",
    "npm": "8.4.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "path": "^0.12.7"
  },
  "scripts": {
    "client:install": "cd client && npm install",
    "client": "cd client && npm start",
    "server": "node server/app.js",
    "dev": "concurrently --kill-others-on-fail \"node server/app.js\" \"cd client && npm start\" ",
    "start": "node server/app.js",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d1gallar/youtube-top.git"
  },

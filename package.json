{
  "name": "youtube-top",
  "version": "1.0.0",
  "description": "> A web app that visualizes the most popular videos and channels on Youtube.",
  "main": "/src/app.js",
  "engines": {
    "node": ">=16.13.2",
    "npm": ">=8.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "axios": "^1.2.3",
    "bootstrap-icons": "^1.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node": "^19.4.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "client-install": "cd client && npm install",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" ",
    "tsc": "rimraf dist && npx tsc",
    "server": "concurrently \"npx tsc -w\" \"nodemon --experimental-json-modules dist/app.js\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d1gallar/youtube-top.git"
  },
  "author": "David Gallardo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d1gallar/youtube-top/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
